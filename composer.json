{
  "name": "legrisch/statamic-webhooks",
  "description": "A Statamic addon to register webhooks for Statamic events.",
  "type": "statamic-addon",
  "autoload": {
    "psr-4": {
      "Legrisch\\StatamicWebhooks\\": "src"
    }
  },
  "require": {
    "php": "^8.1",
    "statamic/cms": "^5",
    "mpbarlow/laravel-queue-debouncer": "^3.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Grischa Erbe",
      "email": "grischa.erbe@googlemail.com"
    }
  ],
  "extra": {
    "statamic": {
      "name": "Statamic Webhooks",
      "description": "A Statamic addon to register webhooks for Statamic events."
    },
    "laravel": {
      "providers": [
        "Legrisch\\StatamicWebhooks\\ServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "pixelfear/composer-dist-plugin": true
    }
  }
}
